/**begin.wxss**/
page{
    margin: 0;
    padding: 0;
    width: 100%;
    height: 100%;
    position: relative;
}
.main{
    /* background-color: #fff; */
    /* border-bottom: 5rpx solid #ccc; */
    border: 1rpx solid #ccc;
}
.main .hidden{
    width: 375px; 
    height: 300px;
    margin: 0 auto;
    visibility:hidden;
}
.main .visible{
    width: 375px; 
    height: 300px;
    margin: 0 auto;
    visibility:visible;
}
.starRun{
    flex-direction: row;
    background-color:rgb(43, 160, 206);
    width: 250rpx;
    height: 250rpx;
    border-radius: 50%;
    text-align: center;
    line-height: 250rpx;
    position: fixed;
    bottom: 95rpx;
    left: 50%;
    transform: translateX(-50%);
    z-index: 99;
}
.starRun .star{
    font-size: 60rpx;
    color: greenyellow;
}
.starRun .stop{
    font-size: 60rpx;
    color: #ccc;
}
.overnone{
  display: none;
}
.overdis{
  display: ''
}
.imgsize{
  width: 70%;
  height:70%;
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%,-50%);
}